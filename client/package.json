{
	"name": "naive-nestjs-admin",
	"version": "0.0.0",
	"type": "module",
	"private": true,
	"description": "a template based on Vue3, Vite5, Typescript, and Naive UI.",
	"license": "MIT",
	"keywords": [
		"Vue",
		"Vue3",
		"Vite",
		"Vite5",
		"typescript",
		"TypeScript",
		"naive-ui",
		"NaiveUI",
		"unocss",
		"UnoCSS",
		"admin",
		"chatapp"
